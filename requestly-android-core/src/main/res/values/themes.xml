<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.Rqinterceptorandroidsdk" parent="Theme.MaterialComponents.DayNight.DarkActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/purple_500</item>
        <item name="colorPrimaryVariant">@color/purple_700</item>
        <item name="colorOnPrimary">@color/white</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/teal_200</item>
        <item name="colorSecondaryVariant">@color/teal_700</item>
        <item name="colorOnSecondary">@color/black</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="l">?attr/colorPrimaryVariant</item>
        <!-- Customize your theme here. -->
    </style>


    <!-- Headers -->
    <style name="h1" parent="android:TextAppearance">
        <item name="android:textSize">@dimen/s64</item>
        <item name="android:textStyle">bold</item>
    </style>
    <style name="h2" parent="android:TextAppearance">
        <item name="android:textSize">@dimen/s32</item>
        <item name="android:textStyle">bold</item>
    </style>
    <style name="h3" parent="android:TextAppearance">
        <item name="android:textSize">@dimen/s16</item>
        <item name="android:textStyle">bold</item>
    </style>
    <style name="h4" parent="android:TextAppearance">
        <item name="android:textSize">@dimen/s8</item>
        <item name="android:textStyle">bold</item>
    </style>
    <style name="h5" parent="android:TextAppearance">
        <item name="android:textSize">@dimen/s4</item>
        <item name="android:textStyle">bold</item>
    </style>
</resources>
